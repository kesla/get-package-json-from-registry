    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-preset-es2015": "^6.9.0",
    "semistandard": "^8.0.0",
    "snazzy": "^4.0.0"
  },
  "dependencies": {
    "async-cache-promise": "^2.0.1",
